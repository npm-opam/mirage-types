{
    "name": "mirage-types-actual",
    "exportedEnvVars": {
        "mirage_types_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "2.8.0"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "2.8.0"
        },
        "mirage_types_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "mirage_types_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        }
    },
    "version": "2.8.0",
    "dependencies": {
        "@opam-alpha/cstruct": "*",
        "@opam-alpha/ocamlfind": "*",
        "@opam-alpha/ipaddr": "*",
        "@opam-alpha/io-page": "*",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git",
        "substs": "https://github.com/yunxing/substs.git",
        "@opam-alpha/ocamlbuild": "*",
        "@opam-alpha/lwt": "*",
        "nopam": "https://github.com/yunxing/nopam.git",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && make build-types && make install-types && (opam-installer --prefix=$opam_prefix || true)"
    },
    "peerDependencies": {
        "@opam-alpha/ocaml": ">= 4.02.3"
    }
}
